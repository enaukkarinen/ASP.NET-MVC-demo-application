@model WJOArvioitava.Models.ManageUserViewModel

@using WJOArvioitava.Models;
@using Microsoft.AspNet.Identity;
@{
    ViewBag.Title = "Muuta käyttäjätietoja";
}

<h2>@ViewBag.Title.</h2>

<p class="text-success">@ViewBag.StatusMessage</p>
<div class="row">
    <div class="col-md-12">
        @if (ViewBag.HasLocalPassword)
        {
            @Html.Partial("_MuutaSalasanaPartial")
        }
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
} 